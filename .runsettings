<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <!-- Minimum and Maximum of physical cores (not logical) that should be used -->
  <RunConfiguration>
    <MaxCpuCount>1</MaxCpuCount>
    <ResultsDirectory>.\TestResults</ResultsDirectory>
    <!-- Execution -->
    <!-- Valid values should be any integer between the default value and number of cores on the machine. -->
    <TestSessionTimeout>120000</TestSessionTimeout>
  </RunConfiguration>

  <!-- Adapter Specific sections -->
  <NUnit>
    <Where></Where>
    <WorkDirectory>.\</WorkDirectory>
    <NumberOfTestWorkers>-1</NumberOfTestWorkers>
  </NUnit>
</RunSettings>
