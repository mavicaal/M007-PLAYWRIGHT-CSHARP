{
  "Playwright": {
    "BrowserType": "Chromium",
    "Headless": false,
    "SlowMo": 0,
    "Timeout": 30000,
    "NavigationTimeout": 30000
  },
  "Application": {
    "BaseUrl": "http://www.uitestingplayground.com/",
    "Environment": "Staging"
  },
  "Logging": {
    "LogLevel": "Information",
    "LogPath": "Reports/Logs"
  },
  "Screenshots": {
    "CaptureOnFailure": true,
    "Path": "Reports/Screenshots"
  }
}
